{
  "type": "object",
  "properties": {
    "isMyview": {
      "type": "identifier",
      "control": "exists",
      "selector": ".subtle",
      "selectContains": "Assigned to Me"
    },
    "viewissueslink": {
      "type": "action",
      "control": "exists",
      "selector": "a[href='/mantisbt/view_all_bug_page.php']",
      "selectContains": "View Issues"
    }
  },
  "connect": [
    {
      "match": [
        "isMyview"
      ],
      "action": "viewissueslink"
    }
  ]
}
