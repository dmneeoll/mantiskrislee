{
  "type": "object",
  "properties": {
    "issueslist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 3,
            "control": "property.innerText"
          },
          "summary": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selectIndex": 10,
            "selector": "td",
            "control": "property.innerText"
          },
          "status": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": "property.innerText"
          },
          "severity": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 7,
            "control": "property.innerText"
          },
          "assignedto": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selector": "td",
            "selectIndex": 8,
            "control": "property.innerText"
          },
          "updated": {
            "type": "string",
            "update": {
              "type": "none"
            },
            "selectIndex": 9,
            "selector": "td",
            "control": "property.innerText"
          }
        }
      },
      "update": {
        "type": "none"
      },
      "selectorAll": "#buglist tbody tr[bgcolor]",
      "comment": "issuelist"
    },
    "isviewissues": {
      "type": "identifier",
      "control": "exists",
      "selector": ".floatleft",
      "selectContains": "Viewing Issues"
    },
    "reportissueslink": {
      "type": "action",
      "control": "exists",
      "selector": "a[href='/mantisbt/bug_report_page.php']",
      "selectContains": "Report Issue"
    }
  },
  "connect": [
    {
      "match": [
        "isviewissues"
      ],
      "screens": [
        "viewissueslist"
      ]
    },
    {
      "match": [],
      "action": "reportissueslink"
    }
  ]
}
